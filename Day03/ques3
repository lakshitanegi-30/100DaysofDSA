//Implement linear search to find key k in an array. Count and display the number of comparisons performed.
#include <stdio.h>
int main() {
    
    int n;
    scanf("%d", &n);          // array size
    
    int arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]); // array elements
    }

    int k, c = 0, found = -1;
    scanf("%d", &k);          // key to search

    for (int i = 0; i < n; i++) {
        c++;                  // count comparisons
        if (arr[i] == k) {
            found = i;
            break;
        }
    }

    if (found != -1)
        printf("Found at index %d\n", found);
    else
        printf("Not Found\n");

    printf("Comparisons = %d\n", c);

    return 0;
}
